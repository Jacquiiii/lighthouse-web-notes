\f1\fs18 {\listtext	\uc0\u8226 	}
\f2\fs26 the number\'a0\uc0\u8237 0\uc0\u8236 \
\ls3\ilvl1
\f1\fs18 {\listtext	\uc0\u8226 	}
\f2\fs26 the empty string\'a0\uc0\u8237 ""\uc0\u8236 \
